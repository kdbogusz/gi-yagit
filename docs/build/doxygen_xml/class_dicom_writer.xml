          <declname>precision</declname>
        </param>
        <param>
          <type>double</type>
          <declname>fillValue</declname>
        </param>
        <param>
          <type>int</type>
          <declname>rewriteTagsStrategyId</declname>
        </param>
        <param>
          <type>std::set&lt; std::pair&lt; int, int &gt; &gt;</type>
          <declname>tags</declname>
          <defval>(std::set&lt; std::pair&lt; int, int &gt; &gt;())</defval>
        </param>
