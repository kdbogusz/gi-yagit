        <param>
          <type>std::string</type>
          <declname>filepath</declname>
        </param>
        <param>
          <type>gdcm::File &amp;</type>
          <declname>oldFile</declname>
        </param>
        <param>
          <type>double *</type>
          <declname>gamma</declname>
        </param>
        <param>
          <type>int</type>
          <declname>xNumber</declname>
        </param>
        <param>
          <type>int</type>
          <declname>yNumber</declname>
        </param>
        <param>
